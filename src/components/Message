import "./Message.css";
import { Link } from "react-router-dom";

function Message({ message, msg_id }) {
  return (
    <tr className="Log">
      <td>{message.sent_at}</td>
      <td>
        <Link to={`/messages/${msg_id}`}>{message.topic}</Link>
      </td>
    </tr>
  );
}

export default Message;
